{
  "name": "PokemonStreamTool",
  "version": "1.0.0",
  "description": "An electron GUI to manage Pokemon streams.",
  "main": "index.js",
  "repository": "https://github.com/Readek/Pokemon-Stream-Tool",
  "author": "Readek",
  "license": "MIT",
  "devDependencies": {
    "electron": "^30.1.0",
    "electron-builder": "^25.0.5"
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "Readek.PokeStreamTool.PokeStreamToolGUI",
    "productName": "Pokemon Stream Tool",
    "executableName": "Poke Stream Tool",
    "linux": {
      "target": "AppImage",
      "category": "Game",
      "desktop": {
        "Name": "Pokemon Stream Tool",
        "GenericName": "Stream Tool"
      }
    },
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "ws": "^8.18.0"
  }
}
